<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.guilherme.presentation">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/getherfy"
        android:label="@string/app_name"
        android:roundIcon="@drawable/getherfy"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name="com.guilherme.getherfy.activity.PrimeiraTelaActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.guilherme.getherfy.activity.SalaInfoActivity" />
        <activity android:name="com.guilherme.getherfy.activity.CadastrarActivity"/>
        <activity android:name="com.guilherme.getherfy.activity.LoginActivity" />
        <activity android:name="com.guilherme.getherfy.activity.AbasActivity" />
    </application>

</manifest>